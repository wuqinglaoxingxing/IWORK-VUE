    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "node-sass": "^4.14.1",
    "sass-loader": "^10.0.4",
    "vue-router": "^3.4.9",
    "vue-template-compiler": "^2.6.11"
